{
  "name": "normalize-pkg",
  "description": "Normalize values in package.json.",
  "version": "0.3.0",
  "homepage": "https://github.com/jonschlinkert/normalize-pkg/",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/normalize-pkg",
  "bugs": {
    "url": "https://github.com/jonschlinkert/normalize-pkg/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "array-unique": "^0.2.1",
    "export-files": "^2.1.1",
    "extend-shallow": "^2.0.1",
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.3",
    "map-schema": "^0.1.1",
    "minimist": "^1.2.0",
    "parse-github-url": "^0.3.0",
    "remote-origin-url": "^0.4.0",
    "stringify-author": "^0.1.3",
    "try-open": "^0.1.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-format-md": "^0.1.5",
    "gulp-istanbul": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "mocha": "*"
  },
  "keywords": [
    "config",
    "fix",
    "json",
    "normalize",
    "package",
    "package-json",
    "pkg",
    "properties",
    "values"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "update"
      ]
    },
    "reflinks": [
      "verb"
    ]
  }
}